{
    "name": "nullabe/students-grades-api",
    "type": "project",
    "description": "A RESTFull API to manage students' grades. Written in PHP8, using Symfony5 framework.",
    "authors": [
        {
            "name": "Antoine B",
            "email": "abelluard@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ramsey/uuid": "^4.1",
        "symfony/flex": "^1.11",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/framework-bundle": "*",
        "symfony/yaml": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "symfony/proxy-manager-bridge": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.69",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "*"
    },
    "autoload": {
        "psr-4": {
            "StudentsGradesApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StudentsGradesApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
